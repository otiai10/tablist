{
  "manifest_version": 2,
  "name": "Tab Archive",
  "version": "0.0.1",
  "description": "Gather all tabs and make Markdown/HTML formed list. to create daily report easily (for me)",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["dist/js/background.js"]
  },
  "commands": {
    "open_list": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "Open list of tab URLs"
    },
    "search_tab": {
      "suggested_key": {
        "default": "Ctrl+K"
      },
      "description": "Search tab"
    }
  }
}
